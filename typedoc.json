{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": [
        "./src"
    ],
    "entryPointStrategy": "expand",
    "out": "docs",
    "exclude": [
        "**/node_modules/**",
        "**/*.test.ts",
        "**/*.test.tsx",
        "**/*.spec.ts",
        "**/*.spec.tsx"
    ],
    "excludePrivate": true,
    "excludeProtected": false,
    "excludeExternals": true,
    "readme": "README.md",
    "plugin": [],
    "theme": "default",
    "name": "NEVADO - Documentación Técnica",
    "includeVersion": true,
    "categorizeByGroup": true,
    "sort": [
        "source-order"
    ],
    "navigation": {
        "includeCategories": true,
        "includeGroups": true
    }
}